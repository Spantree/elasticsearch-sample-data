{
  "number_of_shards": 1,
  "settings": {
    "analysis": {
      "filter": {
        "edge_left": {
          "type": "edgeNGram",
          "side": "front",
          "min_gram": 1,
          "max_gram": 20
        },
        "alpha_space": {
          "type": "pattern_replace",
          "pattern": "[^a-zA-Z0-9\\s]",
          "replacement": ""
        },
        "text_delimiter": {
          "type": "word_delimiter",
          "generate_word_parts": true,
          "catenate_words": true,
          "preserve_original": true,
          "stem_english_possessive": true
        },
        "first_name_synonyms": {
          "type": "synonym",
          "synonyms_path": "analysis/first_name.synonyms.txt"
        },
        "snowball_stemmed": {
          "type": "snowball",
          "language": "English"
        }
      },
      "analyzer": {
        "edge_left": {
          "type": "custom",
          "filter": [
            "standard",
            "asciifolding",
            "lowercase",
            "stop",
            "edge_left"
          ],
          "tokenizer": "standard"
        },
        "sortable": {
          "type": "custom",
          "filter": [
            "asciifolding",
            "lowercase",
            "alpha_space"
          ],
          "tokenizer": "keyword"
        },
        "text_delimited": {
          "type": "custom",
          "filter": [
            "standard",
            "asciifolding",
            "lowercase",
            "stop",
            "text_delimiter"
          ],
          "tokenizer": "standard"
        },
        "stemmed": {
          "type": "custom",
          "filter": [
            "standard",
            "asciifolding",
            "lowercase",
            "stop",
            "snowball_stemmed"
          ],
          "tokenizer": "standard"
        },
        "first_name_synonyms": {
          "type": "custom",
          "filter": [
            "standard",
            "asciifolding",
            "lowercase",
            "stop",
            "first_name_synonyms"
          ],
          "tokenizer": "standard"
        }
      }
    }
  },
